    {
      "description": "Change cmd-enter to esc in Microsoft Outlook",
      "manipulators": [
        {
          "type": "basic",
          "from": { "key_code": "return_or_enter", "modifiers": { "mandatory": [ "command" ], "optional": [ "option" ] } },
